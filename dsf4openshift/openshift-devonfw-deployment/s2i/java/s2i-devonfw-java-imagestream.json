{
  "kind": "List",
  "apiVersion": "v1",
  "metadata": {
    "name": "s2i-devonfw-java"
  },
  "items": [
    {
      "kind": "BuildConfig",
      "apiVersion": "v1",
      "metadata": {
        "name": "s2i-devonfw-java"
      },
      "spec": {
        "triggers": [
          {
            "type": "GitHub",
            "github": {
              "secret": "secret"
            }
          },
          {
            "type": "Generic",
            "generic": {
              "secret": "secret"
            }
          },
          {
            "type": "ImageChange",
            "imageChange": {}
          }
        ],
        "source": {
          "type": "Git",
          "git": {
            "uri": "https://github.com/yogeshpahilwan/devonfw-shop-floor",
            "ref": "develop"
          }
        },
        "strategy": {
          "type": "Docker",
          "dockerStrategy": {
            "dockerfilePath": "dsf4openshift/openshift-devonfw-deployment/s2i/java/Dockerfile"
          }
        },
        "output": {
          "to": {
            "kind": "ImageStreamTag",
            "name": "s2i-devonfw-java:latest"
          }
        },
        "resources": {}
      }
    },
    {
      "kind": "ImageStream",
      "apiVersion": "v1",
      "metadata": {
        "name": "s2i-devonfw-java"
      },
      "spec": {
        "tags": [
          {
            "name": "latest",
            "annotations": {
              "description": "The DevonFW for Java S2I image",
              "iconClass": "icon-java",
              "tags": "builder,java,maven,springboot,devonfw,microservices,fatjar",
              "supports": "java:8,maven:3,devonfw:2.4",
              "sampleRepo": "https://github.com/yogeshpahilwan/devonfw-shop-floor/tree/develop/dsf4openshift/openshift-devonfw-deployment/s2i.git",
              "version": "1.0"
            }
          }
        ]
      }
    }
  ]
}
